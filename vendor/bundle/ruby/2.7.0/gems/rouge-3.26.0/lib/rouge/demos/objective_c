@interface Person : NSObject {
  @public
  NSString *name;
  @private
  int age;
}

@property(copy) NSString *name;
@property(readonly) int age;

-(id)initWithAge:(int)age;
@end

NSArray *arrayLiteral = @[@"abc", @1];
NSDictionary *dictLiteral = @{
  @"hello": @"world",
  @"goodbye": @"cruel world"
};
